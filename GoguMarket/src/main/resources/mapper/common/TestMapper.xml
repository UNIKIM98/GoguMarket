<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.goguma.common.mapper.TestMapper">

<select id="selectDealAtch" 
		parameterType="int" 
		resultType="com.goguma.common.vo.AtchVO">
SELECT * FROM ATCH
WHERE ATCH_ID = (SELECT atch_id FROM DEAL
                WHERE dl_no = #{dlNo})
</select>

<select id="selectDeal" parameterType="int" resultType="com.goguma.deal.vo.DealVO">
	SELECT * FROM DEAL
	WHERE dl_no = #{dlNo}
</select>
</mapper>